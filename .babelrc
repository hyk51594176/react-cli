{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-0",
    "react"
  ],
  "plugins": ["transform-runtime", "transform-decorators-legacy", ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]],
  "env": {
    "development":{
      "plugins": ["dva-hmr"]
    }
  }
}
